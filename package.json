{
  "name": "transferwise",
  "version": "2.1.0",
  "description": "A simple client for Wise's API written in Node JS (ES7)",
  "main": "lib/Wise.js",
  "engines": {
    "node": ">=16",
    "npm": "7"
  },
  "jest": {
    "bail": false,
    "globals": {
      "NODE_ENV": "test"
    },
    "testEnvironment": "node",
    "verbose": true,
    "watchPathIgnorePatterns": [
      "<rootDir>/data/",
      "<rootDir>/docs/",
      "<rootDir>/log/",
      "<rootDir>/node_modules/",
      "<rootDir>/public/"
    ]
  },
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/13pass/wise-client.git"
  },
  "keywords": [
    "wise",
    "api",
    "client"
  ],
  "author": "Vincent HETRU",
  "license": "Mozilla Public License 2.0",
  "bugs": {
    "url": "https://github.com/13pass/wise-client/issues"
  },
  "homepage": "https://github.com/13pass/wise-client#readme",
  "dependencies": {
    "axios": "^0.30.0",
    "dotenv": "^10.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^7.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
