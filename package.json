{
  "name": "transferwise",
  "version": "2.0.0",
  "description": "A simple client for Transferwise's API written in Node JS (ES7)",
  "main": "lib/Wise.js",
  "engines": {
    "node": "16",
    "npm": "7"
  },
  "jest": {
    "bail": false,
    "globals": {
      "NODE_ENV": "test"
    },
    "testEnvironment": "node",
    "verbose": true,
    "watchPathIgnorePatterns": [
      "<rootDir>/data/",
      "<rootDir>/docs/",
      "<rootDir>/log/",
      "<rootDir>/node_modules/",
      "<rootDir>/public/"
    ]
  },
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/13pass/transferwise-client.git"
  },
  "keywords": [
    "wise",
    "api",
    "client"
  ],
  "author": "Vincent HETRU",
  "license": "Mozilla Public License 2.0",
  "bugs": {
    "url": "https://github.com/13pass/transferwise-client/issues"
  },
  "homepage": "https://github.com/13pass/transferwise-client#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
